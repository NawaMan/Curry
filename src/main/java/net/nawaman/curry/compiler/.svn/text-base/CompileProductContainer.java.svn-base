package net.nawaman.curry.compiler;

/** Container for Compile Product */
public class CompileProductContainer {
	
	CompileProduct CProduct;

	/** Constructs a compile product container */
	public CompileProductContainer() {}
	/** Constructs a compile product container */
	public CompileProductContainer(CompileProduct pCProduct) { this.CProduct = pCProduct; }
			
	/** Returns the compile product */
	public CompileProduct getCompileProduct() { return this.CProduct; }
	
	/**{@inheritDoc}*/ @Override public String toString() {
		return (this.CProduct == null)?"":this.CProduct.toString();
	}
}
