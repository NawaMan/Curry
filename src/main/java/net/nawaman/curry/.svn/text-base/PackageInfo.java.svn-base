package net.nawaman.curry;

/**
 * Provide information about a Package.
 */
public class PackageInfo extends StackOwnerInfo {
	
	public PackageInfo(Package pPackage) {
		super(pPackage);
	}
	
	/** Returns the Package that this object is helping */
	final public Package getPackage() {
		return (Package)this.SO;
	}

}